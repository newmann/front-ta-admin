{
  "name": "front-alain",
  "version": "2.0.0-beta.3",
  "description": "在ng-alain基础上开发的前端系统",
  "author": "newmann",
  "homepage": "https://github.com/newmann/front-alain",
  "bugs": {
    "url": "https://github.com/newmann/front-alain/issues"
  },
  "license": "MIT",
  "keywords": [
    "delon",
    "antd",
    "ng-zorro-antd",
    "angular",
    "component",
    "front-alain"
  ],
  "scripts": {
    "precommit": "npm run lint-staged",
    "ng": "ng",
    "start": "ng serve -o --proxy-config proxy.conf.js --disable-host-check",
    "hmr": "ng serve -c=hmr --proxy-config proxy.conf.js --disable-host-check",
    "ng:serve": "ng serve -o --disable-host-check",
    "serve:disablecheck": "ng serve -o --disable-host-check",
    "debug": "ng serve -o --proxy-config proxy.conf.js --disable-host-check",
    "build": "ng build --prod --build-optimizer",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false",
    "electron:local": "npm run build && electron .",
    "electron:postinstall": "npx electron-builder install-app-deps",
    "electron:start": " npm-run-all -p ng:serve electron:serve",
    "electron:build": " ng build && npm run electron:tsc",
    "electron:build:prod": "ng build --prod && npm run electron:tsc",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "watch": "ng build --watch --output-path dist/ "
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@antv/data-set": "^0.8.8",
    "@antv/g2": "^3.0.11",
    "@antv/g2-plugin-slider": "^2.0.2",
    "@delon/abc": "^2.0.0-beta.3",
    "@delon/acl": "^2.0.0-beta.3",
    "@delon/auth": "^2.0.0-beta.3",
    "@delon/cache": "^2.0.0-beta.3",
    "@delon/chart": "^2.0.0-beta.3",
    "@delon/form": "^2.0.0-beta.3",
    "@delon/mock": "^2.0.0-beta.3",
    "@delon/theme": "^2.0.0-beta.3",
    "@delon/util": "^2.0.0-beta.3",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "ajv": "^6.4.0",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.5.4",
    "d3": "^5.5.0",
    "dagre-d3": "^0.6.1",
    "date-fns": "^1.29.0",
    "extend": "^3.0.1",
    "file-saver": "^1.3.8",
    "font-awesome": "^4.7.0",
    "moment": "^2.19.3",
    "ng-zorro-antd": "^1.6.0",
    "ngx-countdown": "^3.0.0",
    "ngx-gesture-password": "^1.0.1",
    "ngx-notify": "^1.0.2",
    "ngx-tinymce": "^2.0.0",
    "ngx-ueditor": "^2.0.0",
    "qrious": "^4.0.2",
    "replace": "^1.0.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.2.1",
    "screenfull": "^3.3.1",
    "simple-line-icons": "^2.4.1",
    "sweetalert2": "^7.0.0",
    "swiper": "^4.2.2",
    "weather-icons": "^1.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/d3": "^5.0.0",
    "@types/dagre-d3": "^0.4.37",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jszip": "^3.1.3",
    "@types/mockjs": "^1.0.0",
    "@types/node": "~8.9.4",
    "@types/z-schema": "^3.16.32",
    "codecov": "^3.1.0",
    "codelyzer": "~4.3.0",
    "dotenv": "^5.0.1",
    "editorconfig-tools": "^0.1.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sauce-launcher": "^1.2.0",
    "less": "^3.8.1",
    "lint-staged": "^7.1.0",
    "mockjs": "^1.0.1-beta3",
    "ng-alain": "^2.0.0-beta.3",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.12.1",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "~2.9.2",
    "wait-on": "^2.1.0",
    "webpack-bundle-analyzer": "^2.11.2",
    "xlsx": "^0.12.12"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "src/**/*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ]
  }
}
